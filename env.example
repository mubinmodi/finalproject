# OpenAI API Key (for agents)
OPENAI_API_KEY=your_openai_api_key_here

# Tesseract OCR path (if not in system PATH)
TESSERACT_CMD=/usr/local/bin/tesseract

# SEC EDGAR User Agent (required by SEC)
# Format: Your Name your.email@domain.com
SEC_USER_AGENT=YourName your.email@example.com

# Data directories (optional, defaults in config.py)
DATA_RAW_DIR=data/raw
DATA_PROCESSED_DIR=data/processed
DATA_FINAL_DIR=data/final

# Logging level
LOG_LEVEL=INFO

# DVC remote (optional)
# DVC_REMOTE_URL=s3://your-bucket/path
